{
  "name": "ng2handson20160724-ng2-5minquickstart",
  "version": "1.0.0",
  "description": "Angular2ハンズオン(2016年7月24日)向けの公式\"5 min quickstart\"ちょいアレンジ版",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "babel": "babel .toBabel --out-dir .dest --presets es2015 --source-maps",
    "babel:w": "babel .toBabel --out-dir .dest --presets es2015 --source-maps -w",
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "asyncawait": "npm run tsc && npm run babel && concurrently -k \"npm run tsc:w\" \"npm run babel:w\" \"npm run lite\" ",
    "lite": "lite-server --config config/browsersync.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovrmrw/ng2handson20160724-ng2-5minquickstart.git"
  },
  "keywords": [],
